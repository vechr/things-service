{
  "name": "things-service",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "release": {
    "branches": [
      "master"
    ]
  },
  "license": "UNLICENSED",
  "scripts": {
    "commit": "cz",
    "build": "rm -rf .build && nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "npx eslint \"src/**/*.ts\"",
    "lint:fix": "yarn lint --fix",
    "watch": "nest start --debug 0.0.0.0:9229 --watch",
    "dev": "nest start",
    "db:studio": "prisma studio --schema './database/schema.prisma'",
    "db:migrations": "prisma migrate dev --schema './database/schema.prisma' --create-only --name",
    "db:migrate": "prisma migrate dev --schema './database/schema.prisma'",
    "prisma:sync": "prisma generate --schema './database/schema.prisma'",
    "prepare": "husky",
    "prisma:prod:deploy": "prisma migrate deploy --schema './database/schema.prisma'",
    "prestart:prod": "npm run prisma:prod:deploy",
    "start:prod": "node .build/server",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.3.10",
    "@nestjs/core": "^10.3.10",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/microservices": "^10.3.10",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.10",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/terminus": "^10.2.3",
    "@opentelemetry/context-async-hooks": "^1.25.1",
    "@opentelemetry/exporter-prometheus": "^0.52.1",
    "@opentelemetry/exporter-trace-otlp-http": "^0.52.1",
    "@opentelemetry/instrumentation-express": "^0.41.0",
    "@opentelemetry/instrumentation-nestjs-core": "^0.39.0",
    "@opentelemetry/instrumentation-winston": "^0.39.0",
    "@opentelemetry/propagator-b3": "^1.25.1",
    "@opentelemetry/propagator-jaeger": "^1.25.1",
    "@opentelemetry/sdk-node": "^0.52.1",
    "@opentelemetry/sdk-trace-base": "^1.25.1",
    "@prisma/client": "^5.17.0",
    "@prisma/instrumentation": "^5.17.0",
    "@redis/client": "^1.5.17",
    "@redis/json": "^1.0.6",
    "cache-manager": "^5.7.3",
    "cache-manager-redis-yet": "^5.1.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.6",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.12",
    "dotenv": "^16.4.5",
    "env-var": "^7.5.0",
    "express-winston": "^4.2.0",
    "glob": "^11.0.0",
    "jsonwebtoken": "^9.0.2",
    "md5": "^2.3.0",
    "ms": "^2.1.3",
    "nats": "^2.28.1",
    "nest-winston": "^1.10.0",
    "nestjs-otel": "^6.1.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "randomstring": "^1.3.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.2.0",
    "short-uuid": "^5.2.0",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.13.1",
    "winston-loki": "^6.1.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.2",
    "@nestjs/schematics": "^10.1.3",
    "@nestjs/testing": "^10.3.10",
    "@types/cookie-parser": "^1.4.7",
    "@types/crypto-js": "^4.2.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/ms": "^0.7.34",
    "@types/node": "20.14.12",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/randomstring": "^1.3.0",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.1.3",
    "jest": "29.7.0",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.3",
    "prisma": "^5.17.0",
    "semantic-release": "^24.0.0",
    "source-map-support": "^0.5.20",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.3",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
