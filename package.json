{
  "name": "things-service",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "rm -rf .build && nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "watch": "nest start --debug 0.0.0.0:9229 --watch",
    "dev": "nest start",
    "db:studio": "prisma studio --schema './database/schema.prisma'",
    "db:migrations": "prisma migrate dev --schema './database/schema.prisma' --create-only --name",
    "db:migrate": "prisma migrate dev --schema './database/schema.prisma'",
    "prisma:sync": "prisma generate --schema './database/schema.prisma'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@nestjs/common": "^8.4.3",
    "@nestjs/core": "^8.4.3",
    "@nestjs/microservices": "^8.4.4",
    "@nestjs/platform-express": "^8.4.7",
    "@nestjs/swagger": "^5.2.1",
    "@prisma/client": "^3.11.1",
    "@types/dotenv": "^8.2.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.1",
    "env-var": "^7.1.1",
    "nats": "^2.6.1",
    "nestjs-pino": "^3.1.1",
    "pino": "^7.9.2",
    "pino-http": "^6.6.0",
    "pino-pretty": "^7.6.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.5.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.4",
    "@nestjs/schematics": "^9.0.1",
    "@nestjs/testing": "^8.4.7",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.1",
    "@types/node": "^16.0.0",
    "@types/pino": "^7.0.5",
    "@types/pino-pretty": "^4.7.5",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.33.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "prisma": "^4.1.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.2.4",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.7.4",
    "husky": "^8.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
